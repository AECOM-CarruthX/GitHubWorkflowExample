<Application x:Class="MVVMDemoNew.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MVVMDemoNew"
             xmlns:catalog="clr-namespace:MVVMDemoNew.Catalog"
             xmlns:cart="clr-namespace:MVVMDemoNew.Cart">
    <Application.Resources>
        <DataTemplate DataType="{x:Type catalog:CatalogViewModel}">
            <catalog:CatalogView />
        </DataTemplate>
        <DataTemplate DataType="{x:Type cart:CartViewModel}">
            <cart:CartView />
        </DataTemplate>
    </Application.Resources>
</Application>
